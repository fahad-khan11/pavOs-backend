{
  "name": "paveos-be",
  "version": "1.0.0",
  "description": "PaveOS Backend - CRM and automation app for creators",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "seed": "ts-node --esm scripts/seedDemo.ts",
    "seed:templates": "ts-node --esm src/scripts/seedTemplates.ts",
    "create-demo": "ts-node --esm scripts/createDemoUser.ts",
    "test:threads": "ts-node --esm scripts/testThreadImplementation.ts",
    "test:reconnect": "ts-node --esm scripts/testReconnectFix.ts",
    "fix-stale-connections": "ts-node --esm scripts/fixStaleConnections.ts",
    "cleanup-test-data": "ts-node --esm scripts/cleanupTestData.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "crm",
    "whop",
    "stripe",
    "automation"
  ],
  "author": "PaveOS Team",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.13.0",
    "@discordjs/rest": "^2.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@whop/sdk": "^0.0.8",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "discord-oauth2": "^2.12.1",
    "discord.js": "^14.24.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^4.8.1",
    "stripe": "^17.6.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "uuid": "^11.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.3.15",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
